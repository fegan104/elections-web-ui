{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/v1/**",
        "run": {
          "serviceId": "scottish-stv-api",
          "region": "us-central1"
        }
      }
    ]
  }
}
